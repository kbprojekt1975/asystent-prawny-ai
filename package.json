{
  "name": "cloud-functions-asystent-prawny-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "emulators": "firebase emulators:start --import=./emulator_data",
    "emulators:clean": "firebase emulators:start",
    "build:functions": "npm --prefix functions run build",
    "update-pricing": "npm run build:functions && node functions/lib/scripts/updatePricing.js",
    "seed": "node functions/seed_config.cjs --emulator",
    "seed:prod": "node functions/seed_config.cjs --prod",
    "watch:functions": "npm --prefix functions run build:watch"
  },
  "dependencies": {
    "firebase": "^12.6.0",
    "i18next": "^25.7.4",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.5.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}